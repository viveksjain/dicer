# proto-file: dicer/common/test/proto/test_data.proto
# proto-message: FatalTargetMismatchTestDataP

# Test cases where the targets are considered fatally mismatched.
fatally_mismatched_test_cases: [
  {
    description: "Both Kubernetes targets, name mismatch"
    target1: {
      name: "name1"
      kubernetes_cluster_uri: "kubernetes-cluster:test-env/cloud1/public/region1/clustertype2/01"
      target_type: KUBERNETES
    }
    target2: {
      name: "name2"
      kubernetes_cluster_uri: "kubernetes-cluster:test-env/cloud1/public/region1/clustertype2/01"
      target_type: KUBERNETES
    }
  },
  {
    description: "Both Kubernetes targets, both name and cluster mismatch"
    target1: {
      name: "name1"
      kubernetes_cluster_uri: "kubernetes-cluster:test-env/cloud1/public/region1/clustertype2/01"
      target_type: KUBERNETES
    }
    target2: {
      name: "name2"
      kubernetes_cluster_uri: "kubernetes-cluster:test-env/cloud1/public/region1/clustertype1/kjfna2"
      target_type: KUBERNETES
    }
  },
  {
    description: "Both App targets, name mismatch"
    target1: {
      name: "name1"
      target_type: APP
      instance_id: "instance-id1"
    }
    target2: {
      name: "name2"
      target_type: APP
      instance_id: "instance-id1"
    }
  },
  {
    description: "Both App targets, instance ID mismatch"
    target1: {
      name: "name1"
      target_type: APP
      instance_id: "instance-id1"
    }
    target2: {
      name: "name1"
      target_type: APP
      instance_id: "instance-id2"
    }
  },
  {
    description: "Both App targets, both name and instance ID mismatch"
    target1: {
      name: "name1"
      target_type: APP
      instance_id: "instance-id1"
    }
    target2: {
      name: "name2"
      target_type: APP
      instance_id: "instance-id2"
    }
  },
  {
    description: "Type mismatch: Kubernetes vs App"
    target1: {
      name: "name1"
      kubernetes_cluster_uri: "kubernetes-cluster:test-env/cloud1/public/region1/clustertype2/01"
      target_type: KUBERNETES
    }
    target2: {
      name: "name1"
      target_type: APP
      instance_id: "instance-id1"
    }
  }
]

