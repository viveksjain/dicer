invalid_uri_test_cases: [
  {
    description: "Invalid scheme"
    cluster_uri: "http://kubernetes-cluster:prod/cloud1/public/region1/clustertype2/01"
    expected_error_message: "Expected a kubernetes-cluster URI, got: http://kubernetes-cluster:prod/cloud1/public/region1/clustertype2/01"
  },
  {
    description: "Invalid path"
    cluster_uri: "kubernetes-cluster:prod/cloud1/public/region1/gc"
    expected_error_message: "Cluster URI path must have 6 parts: parts=[prod, cloud1, public, region1, gc]"
  }
]

